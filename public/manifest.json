{
  "name": "VIN Multiplier",
  "version": "1.0.1",
  "manifest_version": 3,
  "description": "Zahl markieren und berechnen lassen.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}